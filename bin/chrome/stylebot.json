{
    "credio.com": {
        "_enabled": true,
        "_rules": {
            ".BOX-mask": {
                "opacity": "0"
            },
            ".BOX-open": {
                "overflow-y": "visible"
            },
            ".ad-block-pop-up": {
                "display": "none"
            }
        }
    },
    "facebook.com": {
        "_enabled": true,
        "_rules": {
            "div#pagelet_growth_expanding_cta": {
                "display": "none"
            },
            "div#u_0_3": {
                "display": "none"
            }
        }
    },
    "funimation.com": {
        "_enabled": true,
        "_rules": {
            ".funimation-overlay": {
                "display": "none"
            },
            ".vjs-contextmenu-ui-menu": {
                "display": "none",
                "position": "absolute"
            }
        }
    },
    "linkedin.com": {
        "_enabled": true,
        "_rules": {
            "#ad": {
                "display": "none"
            },
            "#advocate-modal": {
                "background": "none"
            },
            "#advocate-modal .modal-content": {
                "display": "none"
            },
            ".reg-upsell": {
                "display": "none"
            },
            "body.advocate-modal-visible": {
                "overflow": "initial"
            }
        }
    },
    "youtube.com": {
        "_enabled": true,
        "_rules": {
            "span#musixmatch-logo": {
                "display": "none"
            }
        }
    }
}
